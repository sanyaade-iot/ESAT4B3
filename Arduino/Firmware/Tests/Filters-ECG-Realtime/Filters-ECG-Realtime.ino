void setup() {
  Serial.begin(115200);
  pinMode(LED_BUILTIN, OUTPUT);
}

void loop() {
#if defined(ECG_SENSOR) || defined(ECG_DATA)
  static uint16_t ECG_index = 0;
  static unsigned long ECG_prevmicros = micros();
  if (!Serial) {
    ECG_prevmicros = micros();
    ECG_index = 0;
  } else if (micros() - ECG_prevmicros >= ECG_interval) {
    unsigned long startTime = micros();
    ECG_prevmicros += ECG_interval;
#ifdef ECG_SENSOR
    int16_t value = analogRead(ECG_pin);
#elif defined(ECG_DATA)
    int16_t value = pgm_read_word_near(ECG_data + ECG_index);
    ECG_index = (ECG_index + 1) % ECG_len;
#endif
    float filteredf = notch.filter(lp.filter(hp.filter(value - DC_offset)));
    int16_t filtered = round(filteredf) + DC_offset;
    int distance = getPeakDistance(filtered);
    int BPM = 0;
    if (distance == -1) {
      BPM = -300; // Patient is dead
    } else if (distance) {
      BPM = 60 * 360 / distance;
    }
#ifdef USE_PROTOCOL
    send(filtered, ECG);
#else
#ifdef LOG_CPU_USAGE
    // encode(messageToSend, filtered, ECG);
    float CPU_usage = CPU_usage_average.add(float(micros() - startTime) * 100.0 / ECG_interval);
    Serial.print(CPU_usage);
    Serial.print('\t');
#endif
    Serial.print(filtered);
    Serial.print('\t');
    Serial.print(value);
    Serial.print('\t');
    Serial.print(BPM);

    Serial.println();
    Serial.flush();
#endif
  }
#endif
}

int getPeakDistance(uint16_t value) {
  static unsigned int ctr, prevCtr = 0;
  static uint16_t max = 0;

  int distance = 0;
  if (prevCtr > 0)
    prevCtr++;

  if (max > 0) {
    if (value >= max) { // New maximum
      ctr = 0;
      max = value;
      digitalWrite(LED_BUILTIN, HIGH);
    } else if (value < threshold) { // End of peak
      ctr++;
      if (prevCtr > 0) // there was a previous peak
        distance = prevCtr - ctr; // calculate the difference from the previous peak
      prevCtr = ctr;
      max = 0;
      digitalWrite(LED_BUILTIN, LOW);
    } else {
      ctr++;
      digitalWrite(LED_BUILTIN, LOW);
    }
  } else if (prevCtr >= maxdistance) { // Previous peak was too long ago, forget it
    prevCtr = 0;
    distance = -1;
  } else if (value >= threshold) {
    ctr = 0;
    max = value;
  }
  return distance;
}

